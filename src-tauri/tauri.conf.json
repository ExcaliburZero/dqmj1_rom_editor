{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "dqmj1_rom_editor",
  "version": "0.1.1",
  "identifier": "com.dqmj1-rom-editor.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "DQMJ1 ROM Editor",
        "width": 900,
        "height": 550
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
